{
  "name": "practice-tool-rescript-frontend",
  "packageManager": "yarn@4.10.3",
  "scripts": {
    "res:build": "rescript clean && rescript",
    "res:dev": "rescript clean && rescript build -w",
    "dev": "next dev",
    "intl": "node ./script/intlExtractor.js",
    "prepare:dev": "node ./script/lib/js/aws/Aws_PrepareBuild_Dev.res.js",
    "prepare:prod": "node ./script/lib/js/aws/Aws_PrepareBuild_Prod.res.js",
    "build": "next build",
    "deploy:dev": "node ./script/lib/js/aws/Aws_Deploy_Dev.res.js",
    "deploy:prod": "node ./script/lib/js/aws/Aws_Deploy_Prod.res.js",
    "deploy:complete:dev": "yarn res:build && yarn intl && yarn prepare:dev && yarn build && yarn deploy:dev",
    "deploy:complete:prod": "yarn res:build && yarn intl && yarn prepare:prod && yarn build && yarn deploy:prod",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "@dck/restorative": "^1.1.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@greenlabs/ppx-spice": "0.2.2",
    "@mui/icons-material": "^6.1.8",
    "@mui/material": "^6.1.8",
    "@mui/material-nextjs": "^6.1.8",
    "@mui/x-date-pickers": "^7.22.3",
    "@rescript/core": "^1.6.1",
    "@serwist/next": "^9.2.1",
    "dayjs": "^1.11.18",
    "next": "^15.5.6",
    "oidc-client-ts": "^3.3.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "react-intl": "^7.1.14",
    "react-oidc-context": "^3.3.0"
  },
  "devDependencies": {
    "@greenlabs/rescript-react-hook-form": "^7.31.0",
    "@rescript-mui/material": "^5.0.0",
    "@rescript/react": "^0.13.0",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "rescript": "^11.1.4",
    "rescript-react-intl": "^3.0.0",
    "rescript-react-intl-extractor-bin": "^0.14.0",
    "rescript-webapi": "^0.10.0",
    "serwist": "^9.2.1",
    "typescript": "^5.9.3"
  }
}
