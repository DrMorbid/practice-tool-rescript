{
 "name": "practice-tool-frontend",
 "sources": [
  {
   "dir": "src",
   "subdirs": true
  }
 ],
 "package-specs": [
  {
   "module": "esmodule",
   "in-source": true
  }
 ],
 "suffix": ".res.js",
 "bs-dependencies": [
  "@rescript/react",
  "@rescript/core",
  "@rescript-mui/material",
  "rescript-webapi",
  "rescript-react-intl",
  "@greenlabs/ppx-spice",
  "@dck/restorative"
 ],
 "bsc-flags": [
  "-open RescriptCore"
 ],
 "jsx": {
  "version": 4,
  "mode": "automatic"
 },
 "ppx-flags": [
  [
   "@greenlabs/ppx-spice/ppx",
   "-uncurried"
  ]
 ]
}