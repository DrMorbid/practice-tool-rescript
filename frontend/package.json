{
  "name": "practice-tool-rescript-frontend",
  "packageManager": "yarn@4.5.0",
  "scripts": {
    "res:build": "rescript clean && rescript",
    "res:dev": "rescript clean && rescript build -w",
    "dev": "next dev",
    "intl": "node ./script/intlExtractor.js",
    "prepare:dev": "node ./script/lib/js/aws/Aws_PrepareBuild_Dev.res.js",
    "prepare:prod": "node ./script/lib/js/aws/Aws_PrepareBuild_Prod.res.js",
    "build": "next build",
    "deploy:dev": "node ./script/lib/js/aws/Aws_Deploy_Dev.res.js",
    "deploy:prod": "node ./script/lib/js/aws/Aws_Deploy_Prod.res.js",
    "deploy:dev:complete": "yarn res:build && yarn intl && yarn prepare:dev && yarn build && yarn deploy:dev",
    "deploy:prod:complete": "yarn res:build && yarn intl && yarn prepare:prod && yarn build && yarn deploy:prod",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "@dck/restorative": "^1.1.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@greenlabs/ppx-spice": "^0.2.2",
    "@mui/icons-material": "^6.1.1",
    "@mui/material": "^6.1.1",
    "@mui/material-nextjs": "^6.1.1",
    "@serwist/next": "^9.0.9",
    "next": "^14.2.14",
    "oidc-client-ts": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-intl": "^6.7.0",
    "react-oidc-context": "^3.1.1"
  },
  "devDependencies": {
    "@greenlabs/rescript-react-hook-form": "^7.31.0",
    "@rescript-mui/material": "^5.0.0-rc.2",
    "@rescript/core": "^1.6.0",
    "@rescript/react": "^0.13.0",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "rescript": "^11.1.4",
    "rescript-react-intl": "^3.0.0",
    "rescript-react-intl-extractor-bin": "^0.14.0",
    "rescript-webapi": "^0.9.1",
    "serwist": "^9.0.9",
    "typescript": "^5.6.2"
  }
}
